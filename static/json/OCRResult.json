[{"line": 0, "text": "\u0627\u0648\u0644\u06ad\u0632 \u0628\u0642 : \u0628\u0631 \u0628\u0648\u06cc\u0648\u0643 \u0627\u062b\u0631\u062f\u0631 \u062c \u0642\u0637\u0639\u0647 \u06cc\u0647 \u0628\u0631 \u0641\u0644\u0633\u0641\u0647 \u0635\u0633\u063a\u0645\u0634\u062f\u06cc\u0631 \u062f \u0628\u06cc\u0644\u0645\u0634 \u0668", "coord": [84, 1204]}, {"line": 1, "text": "\u0627\u0648\u0646\u06cc. \u0645\u0644\u062a\u06ad \u0644\u0633\u0627\u0646\u06cc\u0644\u0647 \u0633\u0648\u06cc\u0644\u06cc\u0647 \u0628\u06cc\u0644\u0645\u0634\u0633\u06ad\u0632 \u0642 \u0645\u0648\u0641\u0642\u06cc\u062a\u0643\u0632 \u0634\u0627\u06cc\u0627\u0646 \u062a\u0631\u0643\u062f\u0631 \u2014", "coord": [91, 1204]}, {"line": 2, "text": "\u062f\u06cc\u0646 \u0627\u0633\u0644\u0627\u0645\u06ad \u0638\u0647\u0648\u0631 \u0627\u0634. \u0646\u062f\u0646 \u0627\u0644 \u2014 \u062d\u062a\u06cc \u0686\u0648\u0642 \u0644 \u0627\u0632\u0646", "coord": [113, 1204]}, {"line": 3, "text": "\u0628\u0634 \u0645\u06af\u0631 \u0645\u06cc \u0633\u0646\u0647 \u0627\u0648\u0644 \u2014 \u0642 \u0642 \u0642\u0648\u0645\u0646\u06ad \u062d\u0627\u0644\u06cc \u0634\u0627\u06cc\u0627\u0646 \u062f\u0642\u062a \u0627\u06cc\u062f\u06cc. \u0639\u0631\u0628\u0644\u0631", "coord": [95, 1204]}, {"line": 4, "text": "\u0642\u0632 \u0686\u0648\u062c\u0642\u0644\u0631\u06cc\u0646\u06cc \u062f\u06cc\u0631\u06cc \u062f\u06cc\u0631\u06cc \u0637\u0648\u067e\u0631\u0627\u063a\u0647 \u06af\u0648\u0645\u0631\u0644\u0631 \u0643\u0631 \u0627\u0648\u0644\u0627\u062f \u0645\u0633\u062a\u06cc \u0646\u0647 \u062f\u0631 \u0647\u0686", "coord": [93, 1204]}]